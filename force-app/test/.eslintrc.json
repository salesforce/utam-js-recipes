{
    "extends": ["eslint:recommended", "plugin:jasmine/recommended", "plugin:wdio/recommended"],
    "plugins": ["jasmine", "wdio"],
    "env": {
        "node": true,
        "es2021": true,
        "jasmine": true
    },
    "globals": {
        "utam": "readonly"
    },
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": "latest"
    },
    "rules": {
        "jasmine/new-line-before-expect": "off"
    }
}
